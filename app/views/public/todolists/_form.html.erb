<% if user_signed_in? %>
<div class="form-set">
	<div class="todolist-form">
		<h4>Todoリスト作成</h4>
		<%= form_with model: [:public, todolist], local: true do |f| %>
		<%= render 'shared/error_messages', obj: todolist %>
		<div class="form-group">
			<%= f.label :内容 %>
			<%= f.text_field :content %>
		</div>
		<div class="form-group">
			<%= f.label :ラジオボタン %>
			<%#= f.text_field :name %>
		</div>
		<div class="actions">
			<%= f.submit '作成' %>
		</div>

		<% end %>
	</div>
</div>
<% end %>