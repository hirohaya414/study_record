<div class="todolist-form">

	<%= form_with model: [:public, todolist], local: true do |f| %>
	<%= render 'shared/error_messages', obj: todolist %>
	<div class="form-group">
		<%= f.label :内容 %>
		<%= f.text_field :content %>
	</div>
	<div class="form-group">
		<%= f.label :ラジオボタン %>
		<%#= f.text_field :name %>
	</div>
	<div class="actions">
		<%= f.submit '保存する', class: 'btn btn-outline-info' %>
	</div>

	<% end %>
</div>
